<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IQuestionService" />
        <binding name="BasicHttpBinding_IResourceService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:52160/QuesionService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IQuestionService"
        contract="QuestionService.IQuestionService" name="BasicHttpBinding_IQuestionService" />
      <endpoint address="http://localhost:52160/ResourceService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IResourceService"
        contract="ResourceService.IResourceService" name="BasicHttpBinding_IResourceService" />
    </client>
  </system.serviceModel>
</configuration>